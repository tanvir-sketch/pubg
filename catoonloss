                            if (isLoss) {
                              leaderboardBalance.style.color = "#ff6251";
                              leaderboardBalance.innerHTML = `-${new Intl.NumberFormat('en-US', { minimumFractionDigits: 2, maximumFractionDigits: 2 }).format(absProfit)}$`;  // ‡¶∏‡ßá‡¶®‡ßç‡¶ü‡¶∏‡¶π ‡¶≤‡¶∏ ‡¶¶‡ßá‡¶ñ‡¶æ‡¶¨‡ßá
                            } else {
                              leaderboardBalance.style.color = "#0faf59";
                              leaderboardBalance.innerHTML = formattedBalance;  // ‡¶∏‡ßá‡¶®‡ßç‡¶ü‡¶∏‡¶π ‡¶™‡ßç‡¶∞‡¶´‡¶ø‡¶ü ‡¶¶‡ßá‡¶ñ‡¶æ‡¶¨‡ßá
                            }
